dtmc

module dtmc_model

    s : [0..3] init 0;

    [] s=0 -> 1 : (s'=0) + 0 : (s'=1) + 0 : (s'=2) + 0 : (s'=3);
    [] s=1 -> 1/2 : (s'=0) + 1/6 : (s'=1) + 1/6 : (s'=2) + 1/6 : (s'=3);
    [] s=2 -> 1/34 : (s'=0) + 3/34 : (s'=1) + 1/34 : (s'=2) + 29/34 : (s'=3);
    [] s=3 -> 0 : (s'=0) + 0 : (s'=1) + 0 : (s'=2) + 1 : (s'=3);

endmodule
