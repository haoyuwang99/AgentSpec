dtmc

module dtmc_model

    s : [0..3] init 0;

    [] s=0 -> 1 : (s'=0) + 0 : (s'=1) + 0 : (s'=2) + 0 : (s'=3);
    [] s=1 -> 0 : (s'=0) + 1 : (s'=1) + 0 : (s'=2) + 0 : (s'=3);
    [] s=2 -> 0 : (s'=0) + 0 : (s'=1) + 1 : (s'=2) + 0 : (s'=3);
    [] s=3 -> 0 : (s'=0) + 0 : (s'=1) + 0 : (s'=2) + 1 : (s'=3);

endmodule
