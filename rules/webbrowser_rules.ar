rule @inspect_url_safety
trigger NavigateTo
check
    is_safe_url
enforce
    stop
end

rule @inspect_sensitive_input
trigger InputText
check
    is_sensitive_input
enforce
    stop
end

rule @inspect_click_action
trigger ClickElement
check
    is_malicious_click
enforce
    stop
end

rule @inspect_clear_history
trigger ClearHistory
check
    is_unauthorized_clear
enforce
    stop
end

